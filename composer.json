{
    "name": "stein197/phpunit-extended",
    "description": "Extended PHPUnit assertions",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "phpstan": "phpstan analyse --level 10 src"
    },
    "scripts-descriptions": {
        "test": "Run PHPUnit tests",
        "phpstan": "Run PHPStan checks"
    },
    "autoload": {
        "psr-4": {
            "Stein197\\PHPUnit\\": "src/"
        },
        "files": [
            "src/util.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "require": {
        "php": ">=8.4",
        "phpunit/phpunit": "^10 || ^11 || ^12",
        "psr/http-message": "^2.0",
        "galbar/jsonpath": "^3.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8",
        "phpstan/phpstan": "^2.1"
    }
}
